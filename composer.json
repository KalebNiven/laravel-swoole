{
    "name": "huang-yi/laravel-swoole-http",
    "description": "High performance HTTP server based on Swoole. Speed up your Laravel and Lumen applications.",
    "keywords": ["swoole", "laravel", "lumen", "performance", "http", "server"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "ext-swoole": ">=1.9.3"
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "laravel/framework": "~5.1",
        "laravel/lumen-framework": "~5.1",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Http\\Tests\\": "tests"
        }
    }
}
